{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],
    
        "rewrites": [
          {
            "source": "/fetch-metadata",
            "destination": "http://localhost:5000/fetch-metadata"
          }
        ]
      }
   